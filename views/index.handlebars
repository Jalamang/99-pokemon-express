<h1 class="text-center mb-3">{{title}}</h1>

<h4 class=".bg-secondary">Pokemon</h4>
<ul class="list-group .bg-secondary">
  {{#each pokemon}}
  <li class="list-group-item text-primary .text-dark">Name: {{this.name}}; Image Source: {{this.img}} Type: {{this.type}}</li>
  {{!-- {{<img src="this.img" alt="pokemon">}} --}}
  {{/each}}
</ul>

<a href="/pokemon/" class="btn btn-light mt-4">Visit pokemon API</a>